nitData":"","handlerName":""},{"key":"HasFullPower","handlerName":""},{"key":"PowerReceived","handlerName":""},{"key":"IsShooting","initData":"","handlerName":""},{"key":"BufferLevel","handlerName":""}]}],"code":"//te\n//write your own code here\nboolean isADown;\nboolean isDDown;\nboolean isMDown;\nboolean isQDown;\nboolean isEDown;\nboolean isNDown;\n\nint focusIndex;\n\nvoid keyDown(string str) {\n   println(str);\n   //println(\"key down\");\n   if (str == \"A\") {\n      isADown = true;\n      list testing = [10, \"boring\"];\n      println(testing.1);\n   }\n   else if (str == \"D\") {\n      isDDown = true;\n   }\n   else if (str == \"M\") {\n      isMDown = true;\n   } else if (str == \"Q\") {\n      isQDown = true;\n   } else if (str == \"E\") {\n      isEDown = true;\n   } else if (str == \"N\") {\n      isNDown = true;\n   } else if (str == \"B\") {\n      launch(0);\n   }\n   else if (str == \"Space\") {\n      setIsShooting(\"mainCannon\", true);\n      println(\"found space\");\n   }\n   else if (str == \"1\") {\n      int l1 = getRequestLevel(\"l\");\n      int l2 = getRequestLevel(\"r\");\n      setRequestLevel(\"l\", l2);\n      setRequestLevel(\"r\", l1);\n   }\n   else if (str == \"2\") {\n      focusIndex++;\n      string focusName = \"myShip\";\n      for (int z = 0; z < focusIndex; z++) focusName += \"#\";\n      setCameraFocus(focusName, true);\n   }\n   else if (str == \"3\") {\n      focusIndex--;\n      string focusName = \"myShip\";\n      for (int z = 0; z < focusIndex; z++) focusName += \"#\";\n      setCameraFocus(focusName, true);\n   }  \n   else if (str == \"0\") {\n      setRotationEnginePower(50);\n   }\n   else if (str == \"9\") {\n      string focusName = \"myShip\";\n      for (int z = 0; z < focusIndex; z++) focusName += \"#\";\n      println(floatToStr(getAngularVelocity(focusName)));\n   }\n   else if (str == \"8\") {\n      setExplode(\"centerExplosion\", true);\n   } else if (str == \"7\") {\n      println(intToStr(useThisInt()));\n   }\n   updateEngine();\n}\n\n//update the engine power\nvoid updateEngine() {\n   int power = 30;\n   if (isMDown || isNDown) {\n      float ang;\n      ang = getAngularVelocity(\"myShip\");\n      println(floatToStr(ang));\n      if (ang < -0.1) {\n         setEnginePower(\"r\", power);\n         setEnginePower(\"l\", 0);\n      }\n      else if (ang > 0.1) {\n         setEnginePower(\"l\", power);\n         setEnginePower(\"r\", 0);\n      } else {\n         setEnginePower(\"l\", 0);\n         setEnginePower(\"r\", 0);\n      }\n      \n      float speed = getVelocity(\"myShip\");\n      if (speed > 0.1) {\n         setEnginePower(\"topLeft\", power);\n         setEnginePower(\"topRight\", power);\n      } else {\n         setEnginePower(\"topLeft\", 0);\n         setEnginePower(\"topRight\", 0);\n      }\n   }\n   else if (isADown || isDDown){\n      if (isADown) {\n         setEnginePower(\"r\", power);\n      }\n      else {\n         setEnginePower(\"r\", 0);\n      }\n      if (isDDown) {\n         setEnginePower(\"l\", power);\n      }\n      else {\n         setEnginePower(\"l\", 0);\n      }\n   }\n   else{\n      setEnginePower(\"r\", 0);\n      setEnginePower(\"l\", 0);\n   }\n}\n\nvoid keyUp(string str) {\n   //println(\"keyUp\");\n   if (str == \"A\") {\n      isADown = false;\n   }\n   else if (str == \"D\") {\n      isDDown = false;\n   }\n   else if (str == \"M\") {\n      isMDown = false;\n   } else if (str == \"Q\") {\n      isQDown = false;\n   } else if (str == \"E\") {\n      isEDown = false;\n   } else if (str == \"N\") {\n      isNDown = false;\n   }\n   else if (str == \"Space\") {\n      setIsShooting(\"mainCannon\", false);\n   }\n   else if (str == \"0\") {\n      setRotationEnginePower(0);\n   }\n   \n   updateEngine();\n}\n\nint useThisInt() {\n   println(\"do nothing\");\n}\n\n\nvoid setRotationEnginePower(int x) {\n   for (int i = 0; i < 4; i++) {\n      setEnginePower(\"r\" + intToStr(i), x);\n   }\n}\n\n\nvoid bufferLevelUpdate(float value) {\n    println(\"buffer update\");\n    sleep(1000);\n}\n\nvoid timer(float value) {\n   updateEngine();\n   //println(\"Timer\");\n}\n\nvoid launch(float value) {\n   float time = 1000.0;\n   int numCanons = 24;\n   float dTime = time/numCanons;\n   println(\"dTime=\" + intToStr(dTime));\n   sleep(3000);\n   for (int i = 0; i < numCanons; i++) {\n      println(\"started\" + intToStr(i));\n      setIsShooting(\"c\" + intToStr(i), true);\n      sleep(dTime);\n   }\n}","validated":true}}