":1,"properties":[{"key":"Health","handlerName":""},{"key":"PowerRequested","handlerName":""},{"key":"RequestLevel","initData":"","handlerName":""},{"key":"HasFullPower","handlerName":""},{"key":"PowerReceived","handlerName":""},{"key":"EnginePower","initData":"","handlerName":""}]},{"type":"Engine","name":"f8","x":7,"y":20,"rotation":3,"properties":[{"key":"Health","handlerName":""},{"key":"PowerRequested","handlerName":""},{"key":"RequestLevel","initData":"","handlerName":""},{"key":"HasFullPower","handlerName":""},{"key":"PowerReceived","handlerName":""},{"key":"EnginePower","initData":"","handlerName":""}]},{"type":"Engine","name":"r7","x":9,"y":20,"rotation":1,"properties":[{"key":"Health","handlerName":""},{"key":"PowerRequested","handlerName":""},{"key":"RequestLevel","initData":"","handlerName":""},{"key":"HasFullPower","handlerName":""},{"key":"PowerReceived","handlerName":""},{"key":"EnginePower","initData":"","handlerName":""}]},{"type":"Engine","name":"r8","x":7,"y":11,"rotation":3,"properties":[{"key":"Health","handlerName":""},{"key":"PowerRequested","handlerName":""},{"key":"RequestLevel","initData":"","handlerName":""},{"key":"HasFullPower","handlerName":""},{"key":"PowerReceived","handlerName":""},{"key":"EnginePower","initData":"","handlerName":""}]}],"code":"//write your own code here\nboolean isF;\nboolean isR;\nboolean isW;\nboolean isA;\nboolean isS;\nboolean isD;\nboolean change;\nvoid keyDown(string s) {\n   println(s);\n   if (s == \"F\") {\n      if (isF) {\n         isF = false;\n         turnRight(0);\n         println(\"f off\");\n      } else {\n         isF = true;\n         turnRight(100);\n         println(\"f on\");\n      }\n   } else if (s == \"R\") {\n      if (isR) {\n         isR = false;\n         turnLeft(0);\n         println(\"r off\");\n      } else {\n         isR = true;\n         turnLeft(100);\n         println(\"f on\");\n      }\n   } else if (s == \"Space\") {\n      println(intToStr(getAngularVelocity(\"core\")));\n   } else if (s == \"Q\") {\n      for (int i = 0; i < 8; i++) {\n         if (random()*100.0 >= 50.0) {\n            int number = random()*9;\n            println(\"r\" + intToStr(number));\n            setEnginePower(\"r\" + intToStr(number), random()*100.0);\n         } else {\n            int number = random()*9;\n            println(\"f\" + intToStr(number));\n            setEnginePower(\"f\" + intToStr(number), random()*100.0);\n         }\n      }\n   } else if (s == \"Y\") {\n      println(floatToStr(random()*100));\n   } else if (s == \"W\") {\n      isW = true;\n   } else if (s == \"A\") {\n      isA = true;\n   } else if (s == \"S\") {\n      isS = true;\n   } else if (s == \"D\") {\n      isD = true;\n   } else if (s == \"I\") {\n      timer();\n   }\n   change = true;\n   //update();\n}\n\nvoid keyUp(string s) {\n   if (s == \"W\") {\n      isW = false;\n   } else if (s == \"A\") {\n      isA = false;\n   } else if (s == \"S\") {\n      isS = false;\n   } else if (s == \"D\") {\n      isD = false;\n   }\n   change = true;\n   //update();\n}\n\nvoid turnLeft(float power) {\n   setEnginePower(\"r1\", power);\n   setEnginePower(\"r2\", power);\n   setEnginePower(\"r3\", power);\n   setEnginePower(\"r4\", power);\n   setEnginePower(\"r5\", power);\n   setEnginePower(\"r6\", power);\n   setEnginePower(\"r7\", power);\n   setEnginePower(\"r8\", power);\n}\nvoid turnRight(float power) {\n   setEnginePower(\"f1\", power);\n   setEnginePower(\"f2\", power);\n   setEnginePower(\"f3\", power);\n   setEnginePower(\"f4\", power);\n   setEnginePower(\"f5\", power);\n   setEnginePower(\"f6\", power);\n   setEnginePower(\"f7\", power);\n   setEnginePower(\"f8\", power);\n}\nvoid turnLeftLight(float power) {\n   setEnginePower(\"r5\", power);\n   setEnginePower(\"r6\", power);\n   setEnginePower(\"r7\", power);\n   setEnginePower(\"r8\", power);\n}\nvoid turnRightLight(float power) {\n   setEnginePower(\"f5\", power);\n   setEnginePower(\"f6\", power);\n   setEnginePower(\"f7\", power);\n   setEnginePower(\"f8\", power);\n}\nvoid moveUp(float power) {\n   setEnginePower(\"f4\", power);\n   setEnginePower(\"r2\", power);\n}\nvoid moveDown(float power) {\n   setEnginePower(\"f2\", power);\n   setEnginePower(\"r4\", power);\n}\nvoid moveLeft(float power) {\n   setEnginePower(\"f3\", power);\n   setEnginePower(\"r1\", power);\n}\nvoid moveRight(float power) {\n   setEnginePower(\"f1\", power);\n   setEnginePower(\"r3\", power);\n}\n\nint power;\n\nvoid update() {   \n   float rot = getAngularVelocity(\"core\");\n   if (rot > 0.1) {\n      println(\"correcting right\");\n      turnLeftLight(0);\n      turnRightLight(power);\n   } else if (rot < -0.1) {\n      println(\"correcting left\");\n      turnRightLight(0);\n      turnLeftLight(power);\n   } else {\n      turnLeftLight(0);\n      turnRightLight(0);\n   }\n   if (change) {\n   if (isW) {\n      moveUp(power);\n   } else {\n      moveUp(0);\n   }\n   if (isA) {\n      moveLeft(power);\n   } else {\n      moveLeft(0);\n   }\n   if (isS) {\n      moveDown(power);\n   } else {\n      moveDown(0);\n   }\n   if (isD) {\n      moveRight(power);\n   } else {\n      moveRight(0);\n   }\n   }\n   change = false;\n}\n\nvoid timer() {\n   power = 30;\n   while(true) {\n      update();\n   }\n}","validated":true}}