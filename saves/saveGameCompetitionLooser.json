 update();\n      sleep(10);\n   }\n}\n\nvoid update() {\n   if (shipNum == 0) {\n      \n   } else if (shipNum == 1) {\n      driveSolo();\n   }\n}\n\nvoid driveSolo() {\n   list vel = getVelocity(\"main\");\n   float rot = getRotation(\"main\");\n   float velDir = vel.dx*vel.dx + vel.dy*vel.dy;\n   float flyAngle = atan2(vel.dx, vel.dy);\n   if (flyAngle > rot - 0.08727) {\n      setEnginePower(\"prt\", turnPower);\n      setEnginePower(\"plt\", 0);\n   } else if (flyAngle < rot + 0.08727) {\n      setEnginePower(\"plt\", turnPower);\n      setEnginePower(\"prt\", 0);\n   } else {\n      setEnginePower(\"prt\", 0);\n      setEnginePower(\"plt\", 0);\n   }\n   if (wDown) {\n      if (velDir > 25) {\n         setEnginePower(\"pbr\", 0);\n         setEnginePower(\"pbl\", 0);\n      } else {\n         setEnginePower(\"pbr\", drivePower);\n         setEnginePower(\"pbl\", drivePower);\n      }\n   } else {\n      if (velDir > 0.1) {\n         setEnginePower(\"ptr\", stopPower);\n         setEnginePower(\"ptl\", stopPower);\n      } else {\n         setEnginePower(\"pbr\", 0);\n         setEnginePower(\"pbl\", 0);\n      }\n   }\n   if (aDown) {\n      setEnginePower(\"pbr\", drivePower);\n      setEnginePower(\"pbl\", 0);\n   }\n   if (dDown) {\n      setEnginePower(\"pbl\", drivePower);\n      setEnginePower(\"pbr\", 0);\n   }\n}\n\nvoid keydown(string s) {\n   println(s);\n   if (s == \"B\") {\n      setIsShooting(\"separator\", true);\n      sleep(100);\n      setIsShooting(\"separator\", false);\n   } else if (s == \"W\") {\n      wDown = true;\n   } else if (s == \"A\") {\n      aDown = true;\n   } else if (s == \"S\") {\n      sDown = true;\n   } else if (s == \"D\") {\n      dDown = true;\n   } else if (s == \"P\") {\n      shipNum++;\n      shipNum = shipNum % 2;\n   } else if (s == \"Space\") {\n      setIsShooting(\"cSoloLeft\", true);\n      setIsShooting(\"cSoloRight\", true);\n   }\n}\n\nvoid keyup(string s) {\n   if (s == \"W\") {\n      wDown = false;\n   } else if (s == \"A\") {\n      aDown = false;\n   } else if (s == \"S\") {\n      sDown = false;\n   } else if (s == \"D\") {\n      dDown = false;\n   } else if (s == \"Space\") {\n      setIsShooting(\"cSoloLeft\", false);\n      setIsShooting(\"cSoloRight\", false);\n   }\n}","validated":true}}