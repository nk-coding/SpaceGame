eived","handlerName":""}]},{"type":"Cannon","name":"c3","x":9,"y":16,"rotation":0,"properties":[{"key":"Health","handlerName":""},{"key":"PowerRequested","handlerName":""},{"key":"RequestLevel","initData":"2","handlerName":""},{"key":"HasFullPower","handlerName":""},{"key":"PowerReceived","handlerName":""},{"key":"IsShooting","initData":"","handlerName":""},{"key":"BufferLevel"}]},{"type":"Cannon","name":"c2","x":8,"y":16,"rotation":0,"properties":[{"key":"Health","handlerName":""},{"key":"PowerRequested","handlerName":""},{"key":"RequestLevel","initData":"2","handlerName":""},{"key":"HasFullPower","handlerName":""},{"key":"PowerReceived","handlerName":""},{"key":"IsShooting","initData":"","handlerName":""},{"key":"BufferLevel"}]},{"type":"ComputeCore","name":"ShipCore","x":8,"y":14,"rotation":0,"properties":[{"key":"Health","handlerName":""},{"key":"PowerRequested","handlerName":""},{"key":"RequestLevel","initData":"","handlerName":""},{"key":"HasFullPower","handlerName":""},{"key":"PowerReceived","handlerName":""},{"key":"KeyDown","handlerName":"keydown"},{"key":"KeyUp","handlerName":"keyup"},{"key":"AngularVelocity"},{"key":"Velocity"},{"key":"CameraFocus","initData":"true","handlerName":""},{"key":"InitCallback","handlerName":""}]},{"type":"ShieldGenerator","name":"shieldGenerator","x":8,"y":12,"rotation":0,"properties":[{"key":"Health","handlerName":""},{"key":"PowerRequested","handlerName":""},{"key":"RequestLevel","initData":"1","handlerName":""},{"key":"HasFullPower","handlerName":""},{"key":"PowerReceived","handlerName":""},{"key":"Radius","initData":"80","handlerName":""},{"key":"IsEnabled","initData":"true","handlerName":""},{"key":"BufferLevel"}]},{"type":"Cannon","name":"c1","x":7,"y":15,"rotation":0,"properties":[{"key":"Health","handlerName":""},{"key":"PowerRequested","handlerName":""},{"key":"RequestLevel","initData":"2","handlerName":""},{"key":"HasFullPower","handlerName":""},{"key":"PowerReceived","handlerName":""},{"key":"IsShooting","initData":"","handlerName":""},{"key":"BufferLevel"}]},{"type":"Cannon","name":"c0","x":10,"y":15,"rotation":0,"properties":[{"key":"Health","handlerName":""},{"key":"PowerRequested","handlerName":""},{"key":"RequestLevel","initData":"2","handlerName":""},{"key":"HasFullPower","handlerName":""},{"key":"PowerReceived","handlerName":""},{"key":"IsShooting","initData":"","handlerName":""},{"key":"BufferLevel"}]}],"code":"//write your own code here\n\nboolean isADown;\nboolean isDDown;\nboolean isSpaceDown;\n\nvoid keydown(string key) {\n   println(key);\n   if (key == \"A\") {\n      isADown = true;\n   } else if (key == \"D\") {\n      isDDown = true;\n   } else if (key == \"Space\") {\n      isSpaceDown = true;\n   }\n   updateEnginesAndCannon();\n}\n\nvoid keyup(string key) {\n   println(\"keyup\" + key);\n   if (key == \"A\") {\n      isADown = false;\n   } else if (key == \"D\") {\n      isDDown = false;\n   } else if (key == \"Space\") {\n      isSpaceDown = false;\n   }\n   updateEnginesAndCannon();\n}\n\nvoid updateEnginesAndCannon() {\n   int engineVel = 30;\n   if (isADown) {\n      setEnginePower(\"l1\", engineVel);\n      setEnginePower(\"l2\", engineVel);\n   } else {\n      setEnginePower(\"l1\", 0);\n      setEnginePower(\"l2\", 0);\n   }\n   if (isDDown) {\n      setEnginePower(\"r1\", engineVel);\n      setEnginePower(\"r2\", engineVel);\n   } else {\n      setEnginePower(\"r1\", 0);\n      setEnginePower(\"r2\", 0);\n   }\n   for (int i = 0; i < 4; i++) {\n      setIsShooting(\"c\" + intToStr(i), isSpaceDown);\n      setIsEnabled(\"shieldGenerator\", !isSpaceDown);\n   }\n}","validated":true}}